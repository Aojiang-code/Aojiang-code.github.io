(function($){$(function(){window.addEventListener('click',function(event){var $srcElement=$(event.target);if($srcElement.hasClass('usa-nav-link')||$srcElement.parent().hasClass('usa-nav-link')){return;}
var $navLinks=$('.usa-nav-link[aria-expanded=true]');$navLinks.each(function(i,elem){var $elem=$(elem);var controlSelector=$elem.attr('aria-controls');controlSelector=_isValidCssSelector(controlSelector)?controlSelector:'#'+controlSelector;var $control=$(controlSelector);if($control.length&&$control.attr('aria-hidden')==='false'){$elem.click();}});function _isValidCssSelector(selector){return selector.match(/[\#|\.].*/)!==null;}});});})(jQuery);;